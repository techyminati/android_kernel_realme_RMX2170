&pm6150_gpios {
	dischg_enable: dischg_enable {
		pins = "gpio3";
		function = "normal";
		bias-disable;
		output-high;
	};

	dischg_disable: dischg_disable {
		pins = "gpio3";
		function = "normal";
		bias-disable;
		power-source = <0>;
		output-low;
	};
};

